// tests/member_access.src

class Point {
    public float x;
    public float y;
    
    // Method declaration inside class
    public print() : id; 
};

class Circle {
    private float radius;
    public Point;
};

// Function definition
Point::print() : void 
    do
        write(x);
        write(y);
    end

main
    local
        Point p;
        float temp;
    do
        // Test 1: Member assignment
        p.x = 10.5;
        p.y = 20.0;

        // Test 2: Reading from member
        temp = p.x;

        // Test 3: Method call
        p.print();
    end