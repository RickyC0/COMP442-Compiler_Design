StatementIdTail -> IndiceList StatementRest
IndiceList -> EPSILON
StatementRest -> AssignOp Expr ';'
AssignOp -> '='
Expr -> ArithExpr ExprTail
ArithExpr -> Term AddOpTail
Term -> Factor MultOpTail
Factor -> 'intLit'
MultOpTail -> EPSILON
AddOpTail -> EPSILON
ExprTail -> EPSILON
StatementList -> Statement StatementList
Statement -> 'while' '(' RelExpr ')' StatBlock ';'
RelExpr -> ArithExpr RelOp ArithExpr
ArithExpr -> Term AddOpTail
Term -> Factor MultOpTail
Factor -> 'id' FactorIdTail
FactorIdTail -> IndiceList FactorRest
IndiceList -> EPSILON
FactorRest -> EPSILON
MultOpTail -> EPSILON
AddOpTail -> EPSILON
RelOp -> 'lt'
ArithExpr -> Term AddOpTail
Term -> Factor MultOpTail
Factor -> 'intLit'
MultOpTail -> EPSILON
AddOpTail -> EPSILON
StatBlock -> '{' StatementList '}'
Statement -> 'id' StatementIdTail
StatementIdTail -> IndiceList StatementRest
IndiceList -> EPSILON
StatementRest -> AssignOp Expr ';'
AssignOp -> '='
Expr -> ArithExpr ExprTail
ArithExpr -> Term AddOpTail
Term -> Factor MultOpTail
Factor -> 'id' FactorIdTail
FactorIdTail -> IndiceList FactorRest
IndiceList -> EPSILON
FactorRest -> EPSILON
MultOpTail -> EPSILON
AddOpTail -> AddOp Term AddOpTail
AddOp -> '+'
Term -> Factor MultOpTail
Factor -> 'intLit'
MultOpTail -> EPSILON
AddOpTail -> EPSILON
ExprTail -> EPSILON
StatementList -> Statement StatementList
StatementList -> EPSILON
